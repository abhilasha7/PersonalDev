<apex:page standardController="Account" recordsetvar="Accounts" extensions="AccListController">
    <apex:pageBlock >
        <apex:pageBlockTable value="{!accountList}" var="o">
            <apex:column value="{!o.Id}"/>
            <apex:column value="{!o.Name}"/>
            <apex:column value="{!o.CreatedDate}"/>
        </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>